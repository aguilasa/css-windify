{
  "name": "tailwindify-core",
  "version": "0.0.1",
  "description": "Core library for tailwindify",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
